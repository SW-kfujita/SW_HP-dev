    .breadcrumb.animated
      ul
        li
          a(href=path+"/") TOP
        li
          span スカイウイルの人

    h1.page-title.animated
      span.line
      span.title
        small People of SkyWill
        strong スカイウイルの人

    section.section-staff-top
      .sec-header.animated
        ul.list-filter.wrapper
          li
            a(href=path+"/works/cloud.html").active インフラ･クラウド構築
          li
            a(href=path+"/works/development.html") アプリケーション開発
          li
            a(href=path+"/works/support.html") テクニカルサポート
          li
            a(href=path+"/works/marketing.html") マーケティングサービス
          li
            a(href=path+"/works/consulting.html") ITコンサルティング
      .sec-main
        .sec-inner
          ul.list-interview
            li.animated
