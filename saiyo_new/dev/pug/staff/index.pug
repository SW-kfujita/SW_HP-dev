extends ../_layouts/default

block vars
  -title        = 'スカイウイルで働く人 | 株式会社スカイウイル 採用サイト'
  -description  = '株式会社スカイウイルに就職を希望される皆さまへ、社員についてご紹介します。'
  -keywords     = '社員紹介,採用,中途,インフラ・クラウド構築,アプリケーション開発,システム構築,テクニカルサポート,ITコンサルティング,マーケティングサービス,スカイウイル,skywill'
  -bodyClass    = 'page-staff'
  -path         = '..'

block container
